import math
class Point:
	"Point class"
	def __init__(self, x=0, y=0):
		"Constructor"
		self.x = x
		self.y = y
	
	def getX(self):
		"Get X Coord of Point"
		return self.x
		
	def getY(self):
		return self.y
	
	def distanceTo(self, other):
		dx = self.x - other.x
		dy = self.y = other.y		
		return math.sqrt(dx*dx + dy*dy)
